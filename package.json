{
  "name": "W3GCheck",
  "version": "0.0.1a",
  "description": "W3GCheck",
  "main": "./app/build/app.js",
  "scripts": {
    "dev": "concurrently \"cross-env NODE_ENV=development webpack-dev-server --config webpack/dev.config.js\" \"cross-env NODE_ENV=development electron ./app/src/main/app.js\"",
    "prod": "cross-env NODE_ENV=production webpack --progress --color --config webpack/prod.config.js",
    "start": "npm run prod && electron app/build/app.js",
    "lint": "eslint --ext .jsx,.js app/src/",
    "lint:fix": "eslint --ext .jsx,.js app/src/ --fix",
    "pack": "electron-builder --dir",
    "dist": "npm run prod && electron-builder",
    "test": "jest",
    "test:ci": "yarn lint && jest --coverage",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "anXieTyPB",
  "license": "MIT",
  "build": {
    "productName": "W3GCheck",
    "win": {
      "target": "NSIS"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "directories": {
      "output": "release"
    }
  },
  "keywords": [
    "electron",
    "react",
    "react router v4",
    "redux",
    "webpack 4"
  ],
  "devDependencies": {
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "electron": "^2.0.2",
    "electron-builder": "^20.10.0",
    "electron-devtools-installer": "^2.2.3",
    "electron-remote": "^1.3.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "jest": "^23.4.1",
    "jest-cli": "^23.4.1",
    "node-sass": "^4.9.0",
    "react-hot-loader": "^4.1.2",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "buffer": "^5.1.0",
    "classnames": "^2.2.5",
    "devtron": "^1.4.0",
    "history": "^4.7.2",
    "level": "^4.0.0",
    "leveldown": "^4.0.1",
    "md5": "^2.2.1",
    "pouchdb": "^7.0.0",
    "pouchdb-find": "^7.0.0",
    "pouchdb-memory": "^6.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "recharts": "^1.0.0-beta.10",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-form-material-ui": "^5.0.0-beta.3",
    "redux-thunk": "^2.2.0",
    "typeface-roboto": "^0.0.54",
    "url-loader": "^1.0.1",
    "w3gjs": "https://github.com/anxietypb/w3gjs.git"
  }
}
