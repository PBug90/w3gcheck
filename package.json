{
  "name": "W3GCheck",
  "version": "0.0.1a",
  "description": "W3GCheck",
  "main": "./app/build/app.js",
  "scripts": {
    "dev": "concurrently \"cross-env NODE_ENV=development webpack-dev-server --config webpack/dev.config.js\" \"cross-env NODE_ENV=development electron ./app/src/main/app.js\"",
    "prod": "cross-env NODE_ENV=production webpack --progress --color --config webpack/prod.config.js",
    "start": "npm run prod && electron app/build/app.js",
    "lint": "eslint --ext .jsx,.js app/src/",
    "lint:fix": "eslint --ext .jsx,.js app/src/ --fix",
    "pack": "electron-builder --dir",
    "dist": "npm run prod && electron-builder",
    "test": "jest",
    "test:ci": "yarn lint && jest --coverage",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "anXieTyPB",
  "license": "MIT",
  "build": {
    "productName": "W3GCheck",
    "win": {
      "target": "NSIS"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "directories": {
      "output": "release"
    }
  },
  "keywords": [
    "electron",
    "react",
    "react router v4",
    "redux",
    "webpack 4"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-classes": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "babel-loader": "^8.0.6",
    "concurrently": "4.1.1",
    "connected-react-router": "^5.0.1",
    "copy-webpack-plugin": "5.0.4",
    "cross-env": "5.2.0",
    "css-loader": "3.1.0",
    "electron": "6.0.1",
    "electron-builder": "21.2.0",
    "electron-devtools-installer": "2.2.4",
    "electron-remote": "1.3.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-google": "0.13.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "4.1.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "node-sass": "4.12.0",
    "react-hot-loader": "4.12.10",
    "redux-mock-store": "1.5.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "webpack": "4.39.1",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "3.7.2",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "buffer": "5.2.1",
    "classnames": "2.2.6",
    "devtron": "1.4.0",
    "history": "4.9.0",
    "level": "5.0.1",
    "leveldown": "5.1.1",
    "md5": "2.2.1",
    "pouchdb": "7.1.1",
    "pouchdb-find": "7.1.1",
    "pouchdb-memory": "6.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "^5.0.7",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "5.0.0-alpha.9",
    "recharts": "1.6.2",
    "redux": "4.0.4",
    "redux-form": "^7.4.2",
    "redux-form-material-ui": "5.0.0-beta.3",
    "redux-thunk": "2.3.0",
    "typeface-roboto": "0.0.75",
    "url-loader": "2.1.0",
    "w3gjs": "^1.1.2"
  }
}
