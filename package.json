{
  "name": "w3gcheck",
  "version": "0.0.1a",
  "description": "W3GCheck",
  "main": "./app/build/app.js",
  "scripts": {
    "dev": "concurrently \"cross-env NODE_ENV=development webpack-dev-server --config webpack/dev.config.js\" \"cross-env NODE_ENV=development electron ./app/src/main/app.js\"",
    "prod": "cross-env NODE_ENV=production webpack --progress --color --config webpack/prod.config.js",
    "start": "npm run prod && electron app/build/app.js",
    "lint": "eslint --ext .jsx,.js app/src/",
    "lint:fix": "eslint --ext .jsx,.js app/src/ --fix",
    "pack": "electron-builder --dir",
    "dist": "npm run prod && electron-builder",
    "test": "jest",
    "test:ci": "yarn lint && jest --coverage",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "PBug90",
  "license": "MIT",
  "build": {
    "productName": "W3GCheck",
    "win": {
      "target": "NSIS"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "directories": {
      "output": "release"
    }
  },
  "keywords": [
    "electron",
    "react",
    "react router v4",
    "redux",
    "webpack 4"
  ],
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-classes": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "concurrently": "5.0.0",
    "connected-react-router": "^6.5.2",
    "copy-webpack-plugin": "5.0.4",
    "cross-env": "6.0.3",
    "css-loader": "3.2.0",
    "electron": "9.4.0",
    "electron-builder": "21.2.0",
    "electron-devtools-installer": "2.2.4",
    "electron-remote": "1.3.0",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "4.2.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "node-sass": "4.12.0",
    "react-hot-loader": "4.12.15",
    "redux-mock-store": "1.5.3",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.2",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "buffer": "5.4.3",
    "classnames": "2.2.6",
    "devtron": "1.4.0",
    "history": "4.10.1",
    "md5": "2.2.1",
    "pouchdb": "7.1.1",
    "pouchdb-find": "7.1.1",
    "pouchdb-memory": "6.0.0",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "recharts": "1.7.1",
    "redux": "4.0.4",
    "redux-form": "^8.2.6",
    "redux-thunk": "2.3.0",
    "typeface-roboto": "0.0.75",
    "url-loader": "2.2.0",
    "w3gjs": "^1.3.0"
  }
}
